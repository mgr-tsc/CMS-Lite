{
  "AzureStorage": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "Container": "cms"
  },
  "Database": {
    "Path": "cms.sqlite"
  },
  "Jwt": {
    "Issuer": "CmsLite",
    "Audience": "CmsLiteUsers",
    "ExpirationMinutes": 30,
    "ValidateIssuer": true,
    "ValidateAudience": true,
    "ValidateLifetime": true,
    "ValidateIssuerSigningKey": true
  },
  "Logging": {
    "LogLevel": { "Default": "Information" }
  },
  "AllowedHosts": "*"
}